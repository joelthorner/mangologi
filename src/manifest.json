{
  "manifest_version": 3,
  "name": "mangologi",
  "description": "Development tool",
  "default_locale": "en",
  "options_page": "options.html",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "storage",
    "identity",
    "identity.email",
    "scripting",
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "world": "MAIN",
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScripts/CommerceDataShowData.js"
      ]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScripts/CommerceData.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": []
    }
  ]
}