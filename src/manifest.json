{
  "manifest_version": 3,
  "name": "mangologi",
  "description": "Development tool",
  "default_locale": "en",
  "options_page": "options.html",
  "background": {
    "service_worker": "background/index.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "storage",
    "identity",
    "identity.email"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScripts/EcommerceData.js",
        "contentScripts/actions/containerLinesGuide/index.js",
        "contentScripts/actions/containerLinesGuide/load.js"
      ],
      "css": [
        "contentScripts/actions/containerLinesGuide/index.css"
      ]
    }
  ],
  "devtools_page": "devtools.html",
  "web_accessible_resources": [
    {
      "resources": [
        "content.styles.css",
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": []
    }
  ]
}