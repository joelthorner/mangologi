{
  "manifest_version": 3,
  "name": "mangologi",
  "description": "Development tool",
  "default_locale": "en",
  "options_page": "options.html",
  "background": {
    "service_worker": "background.bundle.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "storage",
    "identity",
    "identity.email",
    "scripting",
    "activeTab",
    "tabs",
    "cookies"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "world": "MAIN",
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScripts/CommerceDataShowData.js"
      ]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScripts/CommerceData.js",
        "contentScripts/actions/containerLinesGuide/index.js",
        "contentScripts/actions/containerLinesGuide/load.js"
      ],
      "css": [
        "contentScripts/actions/containerLinesGuide/index.css"
      ]
    },
    {
      "matches": [
        "*://8x-hk.logicommerce.net/*",
        "*://8x-os.logicommerce.net/*",
        "*://8x.logicommerce.net/*",
        "*://8x.logicommerce.cn/*",
        "*://adm.igd.production/*",
        "*://adm.igd.pre.production/*",
        "*://studio.logicommerce.cloud/*"
      ],
      "js": [
        "contentScripts/LogiInfo.js",
        "contentScripts/lcDeveloperBar/index.js"
      ],
      "css": [
        "contentScripts/lcDeveloperBar/index.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": []
    }
  ]
}